#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
   ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n;
    cin >> n;

    vector<int> v1(n);
    for (int i = 0; i < n; i++) {
        cin >> v1[i];
    }

    vector<int> v2 = v1;
    reverse(v2.begin(), v2.end());

    vector<int> dp(n + 1, 0);

    for (int i = 1; i <= n; i++) {
        int pre = 0;
        for (int j = 1; j <= n; j++) {
            int temp = dp[j];

            if (v1[i - 1] == v2[j - 1]) {
                dp[j] = pre + 1;
            } else {
                dp[j] = max(dp[j], dp[j - 1]);
            }
            pre = temp;
        }
    }
    cout << n - dp[n];
}
